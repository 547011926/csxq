  .xiangqing {
    h1 {
      color: $borderColor;
      font-size: $onerem;
      margin-top: $height;
    }
    p {
      margin-top: .7rem;
      font-size: .8rem;
    }
    hr {
      display: block;
      margin: $onerem 0;
    }
    a {
      display: $blo-dis;
      position: absolute;
      right: 1rem;
      bottom: 3rem;
      width: 7rem;
      $height: 1.7rem;
      height: $height;
      line-height: $height;
      text-align: center;
      background: $color;
      color: $listBacolor;
      font-size: .8rem;
      @include border-radius(5px);
    }
  }
  @media all and (min-width: 360px) and (max-width: 413px) {
    .title {
      @include fontSize;
    }
    .xiangqing {
      h1 {
        margin-top: $height;
        font-size: $onerem;
      }
      p {
        font-size: $onerem;
        margin-top: .8rem;
      }
      a {
        $height: 2rem;
        height: $height;
        line-height: $height;
        font-size: $onerem;
        @include fontSize;
      }
    }
  }
  @media all and (min-width: 414px) and (max-width: 470px) {
    .title {
      font-size: 1.15rem;
      margin-bottom: 2rem;
    }
    .xiangqing {
      h1 {
        margin-top: 2rem;
        font-size: 1.5rem;
      }
      p {
        font-size: 1.2rem;
        margin-top: $onerem;
      }
      a {
        width: 9rem;
        $height: 2.5rem;
        height: $height;
        line-height: $height;
        font-size: 1.3rem;
      }
    }
  }
}

//以上为我的订单页面

.huodong {
  @mixin padding($padding, $paddings: $padding) {
    padding: $paddings;
  }
  @mixin bacolor($color, $listBacolor: $color) {
    background: $listBacolor;
  }
  @include padding($onerem);
  .banner {
    @include border-radius(5px);
    @include bacolor(white);
    img {
      width: 100%;
      @include border-radius(5px);
    }
  }
  .nav {
    @include border-radius(5px);
    @include bacolor(white);
    @include padding($onerem);
    margin: $listPadding 0;
    img {
      width: 100%;
    }
  }
  .content {
    @include border-radius(5px);
    @include bacolor(white);
    @include padding($onerem);
    position: relative;
    margin-top: 2rem;
    color: #9a9a9a;
    line-height: 24px;
    font-size: .8rem;
    font-weight: bold;
    .tit {
      position: absolute;
      left: 50%;
      top: -30px;
      margin-left: -30px;
      $size: 115px;
      width: $size/2;
      height: $size/2;
      background: #61a1df url("../images/huodong_11.png") no-repeat center;
      background-size: 50%;
      border: 5px solid #e9e7e8;
      @include border-radius(50%);
    }
    h1 {
      text-align: center;
      color: #61a1df;
      font-size: 1.5rem;
      padding-top: 2rem;
    }
    h3 {
      font-weight: bold;
      margin: .5rem 0;
    }
    p {
      text-indent: 24px;
    }
    span {
      display: block;
      width: 80%;
      margin: 0 auto;
      text-align: center;
      line-height: 24px;
    }
    .huodongtit {
      height: 1px;
      position: relative;
      margin: 2rem 0;
      @include bacolor(#9a9a9a);
      strong {
        padding: .3rem $listPadding;
        @include bacolor(#9a9a9a);
        color: $listBacolor;
        text-align: center;
        position: absolute;
        left: 50%;
        top: 50%;
        margin: -1rem 0 0 -2.2rem;
        letter-spacing: .2rem;
        font-weight: normal;
      }
    }
  }
  .huodong-fixed {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    @include bacolor(#61a1df);
    a {
      display: inline-block;
      width: 24%;
      text-align: center;
      color: $listBacolor;
      background: url("../images/fenxiang.png") no-repeat;
      background-position: 28px 5px;
      padding: 40px 0 5px 0;
      &#py {
        background-position: -38px 7px;
      }
      &#qqhy {
        background-position: -100px 7px;
      }
      &#pyq {
        background-position: -155px 8px;
      }
    }
  }
  @media all and (min-width: 360px) and (max-width: 413px) {
    .content {
      font-size: $onerem;
      p {
        line-height: 32px;
      }
      span {
        line-height: 32px;
      }
    }
    .huodong-fixed {
      a {
        background-position: 30px 5px;
        padding: 50px 0 10px 0;
        background-size: 25rem;
        font-size: $onerem;
        &#py {
          background-position: -61px 7px;
        }
        &#qqhy {
          background-position: -140px 7px;
        }
        &#pyq {
          background-position: -210px 8px;
        }
      }
    }
  }
  @media all and (min-width: 414px) and (max-width: 475px) {
    .content {
      font-size: 1.3rem;
      p {
        line-height: 36px;
      }
      span {
        width: 94%;
        line-height: 36px;
      }
    }
  }
}

//以上为活动页
.examination {
  .one {
    border: 1px solid #d6d6d6 {
      left: 0px;
      right: 0px;
    }
    background: $listBacolor;
    padding-bottom: .2rem;
    li {
      @mixin position($left: 25px, $top: 15px) {
        background-position: $left $top;
      }
      @include sizing;
      display: inline-block;
      width: 24%;
      cursor: pointer;
      background: url("../images/one.png") no-repeat 25px 15px;
      background-size: 50%;
      text-align: center;
      padding-top: 3.8rem;
      &#one-number {
        @include position(25px, -60px);
      }
      &#one-error {
        @include position(25px, -143px);
      }
      &#one-submit {
        @include position(25px, -219px);
      }
      &:nth-child(even) {
        span {
          background: $color;
          color: $listBacolor;
        }
      }
      span {
        display: block;
        width: 100%;
        background: #ededed;
        color: #666;
        padding: .2rem 0;
      }

    }
  }
}

//以上为考试页

//以下为后退公共样式
@mixin textAlign($map:center){
  text-align: $map;
}

@mixin position($position:relative){
  position: $position;
}
@mixin letterSpacing($letter:1px){
  letter-spacing: $letter;
}

@mixin cursor(){
  cursor: pointer;
}
@mixin fontSize($size: 1rem) {
  font-size: $size;
}
.backOff{
  @include textAlign;
  @include position;
  @include fontSize($onerem);
  @include letterSpacing;
  @include cursor;
  padding: $listPadding 0;
  background: white;
  box-shadow: 0 2px 2px rgba(153,153,153,.5);
  margin-bottom:$listPadding ;
  i{
    @include position(absolute);
    width: 0;
    height: 0;
    border-top: 8px solid transparent;
    border-bottom: 8px solid transparent;
    border-right: 8px solid #4196ea;
    left: 1rem;
    &:last-child{
      border-right-color: white;
      left: 1.1rem;
    }
  }

}
//以上为后退公共样式