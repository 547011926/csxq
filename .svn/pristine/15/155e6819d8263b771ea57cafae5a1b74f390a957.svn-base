                .swiperBox {
                    position: absolute;
                    width: 14.5rem;
                    height: 5.42rem;
                    left: 0;
                    bottom: -6.3rem;
                    background: #FFF;
                    -webkit-box-shadow:0 0 10px #999;  
                    -moz-box-shadow:0 0 10px #999;  
                    box-shadow:0 0 10px #999;
                    border: solid 1px #61a1df;
                    z-index: 2;
                }
                .swiperBox .arr_up {
                    background: url(../images/arrow_up.png) no-repeat;
                    width: 19px;
                    height: 9px;
                    position: absolute;
                    top: -9px;
                    left: 50%;
                    margin-left: -10px;
                    display: block;
                }
                .swiperBox .swiper-container {
                    width: 14.5rem;
                    height: 5.42rem;
                    margin: 0.83rem auto;
                }
                
                .swiperBox .swiper-slide {
                    text-align: center;
                    font-size: 0.75rem;
                    width: 3.625rem !important;
                    height: 4.2rem !important;
                    margin-right: 0 !important;
                    /* Center slide text vertically */
                    display: -webkit-box;
                    display: -ms-flexbox;
                    display: -webkit-flex;
                    display: flex;
                    -webkit-box-pack: center;
                    -ms-flex-pack: center;
                    -webkit-justify-content: center;
                    justify-content: center;
                    -webkit-box-align: center;
                    -ms-flex-align: center;
                    -webkit-align-items: center;
                    align-items: center;
                }
                .swiperBox .swiper-slide img {
                    width: 3.625rem;
                    height: 4.2rem;
                }
                .swiperBox .swiper-button-next,.swiper-button-prev {
                    width: 0.8rem;
                    height: 1.3rem;
                    background-size: cover;
                }
                .mask {
                    width: 100%;
                    height: 100%;
                    position: absolute;
                    z-index: 1;
                    display: none;
                    left: 0;
                    top: 0;
                }
